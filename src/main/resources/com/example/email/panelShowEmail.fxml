<?import javafx.scene.layout.*?>
<?import javafx.stage.*?>

<Stage fx:id="stage" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.email.ShowEmail">
    <scene>
        <Scene>
            <AnchorPane prefHeight="429.0" prefWidth="332.0">
            <children>
               <Label layoutX="31.0" layoutY="23.0" text="Email : " />
               <Label layoutX="29.0" layoutY="79.0" text="Title : " />
               <Label layoutX="31.0" layoutY="182.0" text="Text :" />
