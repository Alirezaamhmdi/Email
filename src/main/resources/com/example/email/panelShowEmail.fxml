               <TextField fx:id="textField_title" editable="false" layoutX="30.0" layoutY="96.0" prefHeight="26.0" prefWidth="271.0" />
               <TextField fx:id="textField_email" editable="false" layoutX="29.0" layoutY="48.0" prefHeight="26.0" prefWidth="271.0" />
                <TextField fx:id="textField_date" editable="false" layoutX="30.0" layoutY="148.0" prefHeight="26.0" prefWidth="271.0" />
               <CheckBox fx:id="checkBox_important" layoutX="28.0" layoutY="396.0" mnemonicParsing="false" onAction="#checkBoxAction" text="Important" />
